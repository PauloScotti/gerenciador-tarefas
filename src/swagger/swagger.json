{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "Devaria API NodeJs"
    },
    "basePATH": "/api",
    "consums": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/login": {
            "post": {
                "summary": "Faz autenticação do usuário e devolve o token de acesso",
                "responses": {
                    "200": {
                        "schema": {
                            "$ref": "#/definitions/LoginSucesso"
                        }
                    },
                    "400": {
                        "schema": {
                            "$ref": "#/definitions/LoginInvalido"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "LoginSucesso": {
            "type": "object",
            "properties": {
                "token": {
                    "type": "string",
                    "exmaple": "f2f3fafefafefdd"
                }
            }
        },
        "LoginInvalido": {
            "type": "object",
            "properties": {
                "status": {
                    "type": "number",
                    "exmaple": 400
                },
                "erro": {
                    "type": "string",
                    "example": "Parâmetros de entrada inválidos"
                }
            }
        }
    }
}